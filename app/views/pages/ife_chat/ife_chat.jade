extend ../../layout

block headerContent
	link(rel="stylesheet",href="/css/ife_nav.css")
	link(rel="stylesheet",href="/css/ife_leftBody.css")
	link(rel="stylesheet",href="/css/ife_sideBody.css")
	link(rel="stylesheet",href="/css/ife_chatbox.css")
	link(rel="stylesheet",href="/css/ife_textarea.css")
	link(rel="stylesheet",href="/css/ife_contacts.css")
	style.
		.ife_wrap{
			width:1000px;
			margin:50px auto 0;
			padding:16px 0 0 0;
			font-size:0;
			background-color:rgba(231,116,3,0.15);
			position:relative;
		}
		.ife_wrap .ife_leftNav{
			min-height:592px;
		}
		.ife_wrap .ife_rightBody{
			width:850px;
			display:inline-block;
		}
		.ife_wrap .ife_rightBody .ife_rightMain{
			width:600px;
			margin-right:10px;
			display:inline-block;
			vertical-align:top;
		}
		.ife_wrap .ife_rightBody .ife_sideMain{
			width:230px;
			margin-right:10px;
			position:relative;
			display:inline-block;
		}
block content
	// 导航部分
	include ../ife_nav
	.ife_wrap
		.ife_leftNav
			.ife_leftBody#J_leftBody.ife_leftBodyActive
				ul
					li
						a.pull-left(href="javascript:void(0);") 首页
						i.pull-right.ife_leftarrow
					li.active
						a.pull-left(href="javascript:void(0);") 消息
						i.pull-right.ife_leftarrow
					li
						a.pull-left(href="javascript:void(0);") 收藏
						i.pull-right.ife_leftarrow
					li
						a.pull-left(href="javascript:void(0);") 赞
						i.pull-right.ife_leftarrow
		.ife_rightBody
			.ife_rightMain
				include ./ife_chatbox
			include ./ife_contacts
block footerContent